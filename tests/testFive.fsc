val i : Int = 20
val j : String = "Hello World"

fib(x: Int) : Int -> {
    if x <= 0 then {
        0
    } else if x == 1 then {
        1
    } else {
        fib(x - 1) + fib(x - 2)
    }
}

fact(n: Int) : Int -> {
    if n <= 1 then {
        1
    } else {
        n * fact(n - 1)
    }
}

print("Fib Answer", fib(i))